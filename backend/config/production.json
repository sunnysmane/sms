{
  "env": "production",
  "server": {
    "routes": {
      "cors": true,
      "timeout": {
        "server": 1200000,
        "socket": 1300000
      }
    },
    "port": 50003,
    "host": "0.0.0.0"
  },
  "database": {
    "mongo": {
      "type": "mongo",
      "name": "SMS_BACKEND_MS_PRODUCTION",
      "host": "database",
      "port": 27017,
      "username": "",
      "password": "",
      "options": {
        "host": "database",
        "port": 27017,
        "dialect": "mongo",
        "underscored": true,
        "timestamps": false,
        "define": {
          "underscored": true,
          "timestamps": false
        },
        "pool": {
          "maxConnections": 20,
          "minConnections": 5,
          "maxIdleTime": 30000
        }
      }
    }
  },
  "logging": {
    "name": "SMS_BACKEND_MS_PRODUCTION",
    "level": "debug",
    "path": "/tmp/node_debug.log"
  },
  "swagger": {
    "host": "0.0.0.0"
  }
}
